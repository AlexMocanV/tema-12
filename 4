#include <iostream>
using namespace std;

int main()
{
    int x,y,z;
    cin>>x>>z;
    y=0;
    do 
    {
        y=y*10+x%10;
        x=int(x/100);
    }while(x!=0);
    
    while(y*z>0 and y%10==z%10)
    {
        y=int(y/10);
        z=int(z/10);
    }
    if(y+z==0)
    {
        cout<<1;
    }
    else
    {
        cout<<0;
    }
    
    return 0;
}
